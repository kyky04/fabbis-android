<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.metamorph.fabis">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:name="id.metamorph.fabis.application.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="id.metamorph.fabis.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name=".activities.BaseActivity" /> -->
        <!-- <activity android:name=".activities.SplashActivity"></activity> -->
        <activity android:name="id.metamorph.fabis.activities.LoginActivity"></activity>
        <activity android:name="id.metamorph.fabis.MainActivity"></activity>
        <activity android:name="id.metamorph.fabis.MainPelatihActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.PemainActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.PosisiActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.MenuDetailActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.PemainPenilaianActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.PemainTerbaikActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.PosisiTerbaikActivity"></activity>
        <activity android:name="id.metamorph.fabis.activities.PemainHasilActivity"></activity>
    </application>

</manifest>